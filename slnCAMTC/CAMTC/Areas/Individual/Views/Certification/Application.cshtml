<div style="position: relative; min-height: 500px; height: 100%">
    <div class="header">
        <span>
            <h4 style="text-align: left; margin: 0px !important;">
                <span>
                    <a ID="lnkBacktoDashboard" href="/">← Back to Home</a>
                </span>
            </h4>
        </span>
        <p style="text-align: center; vertical-align: middle; text-indent: 119px; margin-top: 10px; padding-bottom: 10px;">
            <span class="header-span">California Massage Therapy Council</span>
            <span style="display: block; font-size: 16px;">Application for Certification</span> <span style="display: block; font-size: 15px;"><i>Ver. 2015</i></span>
        </p>
    </div>

    <div class="gvDivUpr">
        <div id="ContentPlaceHolder1_ucCertificationApplication1_divVerifiy">

            <div class="divmain" data-ng-app="IndividualApp" ng-controller="mainCtrl as main">

                @{Html.RenderPartial("_LeftPanelPartial"); }
                @{Html.RenderPartial("_RightPanelPartial"); }

            </div>

        </div>
    </div>
