    <script src="~/Content/Public/js/ebsoft-loader.js"></script>
    <script src="~/Content/AngularJS/angular.js"></script>
    <script src="~/Content/AngularJS/angular-route.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
