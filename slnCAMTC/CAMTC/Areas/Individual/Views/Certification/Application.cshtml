    <script src="~/Scripts/DocumentManager/DocumentManager.js"></script>
    <script src="~/Scripts/CertApplication/SectionManager.js"></script>
    
    <script src="~/Areas/Individual/AppJs/IndividualApp.js"></script>
    <script src="~/Areas/Individual/AppJs/RunIndividualApp.js"></script>
    <script src="~/Areas/Individual/AppJs/Service/ApplicationDataService.js"></script>
