
@{
    Layout = "~/Areas/Individual/Views/Shared/_LayoutBasic.cshtml";
    ViewBag.Title = "Application";
}
@section styles{
    <link href="~/Content/Public/css/button.css" rel="stylesheet" />
    <link href="~/Content/Public/css/NewButton.css" rel="stylesheet" />
    <link href="~/Content/Public/css/OnlineApplication.css" rel="stylesheet" />
