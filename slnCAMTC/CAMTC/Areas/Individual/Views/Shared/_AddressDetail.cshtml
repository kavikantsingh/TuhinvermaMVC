<script type="text/javascript">
    $(document).ready(function () {
        $('#EditMailingbtn').on('click', function () {
            $('#EditMailingbtn').hide();
            $('#divEditMailingAddress').removeAttr('style');
            $('#divEditMailingAddress').attr('style', 'visibility:visible;');
            $('#divGridMailingAddress').attr('style', 'display:none;');
            $('#divGridPhysicalAddress').attr('style', 'display:none;');
        });
        $('#btnSaveMailingAddress').on('click', function () {
            $('#EditMailingbtn').show();
            $('#divEditMailingAddress').removeAttr('style');
            $('#divEditMailingAddress').attr('style', 'display:none;');
            $('#divGridMailingAddress').removeAttr('style');
            $('#divGridMailingAddress').attr('style', 'visibility:visible;');
            $('#divGridPhysicalAddress').removeAttr('style');
            $('#divGridPhysicalAddress').attr('style', 'visibility:visible;');
        });
        $('#btnSaveCancelMailingAddress').on('click', function () {
            $('#EditMailingbtn').show();
            $('#divEditMailingAddress').removeAttr('style');
            $('#divEditMailingAddress').attr('style', 'display:none;');
            $('#divGridMailingAddress').removeAttr('style');
            $('#divGridMailingAddress').attr('style', 'visibility:visible;');
            $('#divGridPhysicalAddress').removeAttr('style');
            $('#divGridPhysicalAddress').attr('style', 'visibility:visible;');
        });
        $('#imgbtnEditContact').on('click', function () {
            $('#trEditContactAddress').attr('style', 'visibility:visible;');;
            $('#trGridContactAddress').attr('style', 'display:none;');
        });
        $('#btnSaveContact').on('click', function () {
            $('#trEditContactAddress').attr('style', 'display:none;');
            $('#trGridContactAddress').show();
        });
        $('#btnSaveCancelContact').on('click', function () {
            $('#trEditContactAddress').attr('style', 'display:none;');
            $('#trGridContactAddress').show();
        });
    });
</script>
