    li {
        display: inline-block;
		list-style: none;
	}
</style>

<link href="~/Content/Theme1/jquery-ui/css/start/jquery-ui-1.8.23.custom.css" rel="stylesheet" />
<link href="~/Content/Public/css/style.css" rel="stylesheet" />
<script src="~/Content/Public/js/ebsoft-loader.js"></script>
<script src="~/Content/Public/js/ebsoft-loader.js" type="text/javascript"></script>
<script src="~/Content/AngularJS/angular.js" type="text/javascript"></script>
    @*<script src="~/Content/AngularJS/ag-grid.min.js"></script>
    <script src="~/Areas/BackOffice/AppJS/app.js"></script>*@


<link href="~/Content/Public/css/NewButton.css" rel="stylesheet" type="text/css" />
<script src="~/Content/Public/js/jquery.mask.min.js" type="text/javascript"></script>

@*<script src="../../Validation/FormValidation.js" type="text/javascript"></script>*@<link href="~/Content/Theme1/css/Individual.css?" rel="stylesheet" type="text/css" />

<link href="~/Content/Theme1/css/button.css" rel="stylesheet" />
<link href="~/Content/Theme1/css/NewButton.css" rel="stylesheet" />
