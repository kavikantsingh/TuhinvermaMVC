<script src="~/Content/AngularJS/CustomPaging/dirPagination.js"></script>

<div data-ng-app="contentApp" class="main" style="position: relative">
    <div class="container primary-content ">
        <div>
            <h3 style="float: left;">
                Configuration
            </h3>
        </div>
        <div class="clear">
        </div>
        <div id="dashboard-panels">
            <div class="relative-box ">
                <div class="dynamic-panels ui-sortable" style="">

                        <div id="divButtonList" class="tabBtnBorder">
                                <input type="button" ng-click="tab = 1" value="Codes" class="tabBtn" />
                                <input type="button" ng-click="tab = 2" value="Upload Document List" class="tabBtn" />
                                <input type="button" ng-click="tab = 3" value="Application Status Color" class="tabBtn" />
                                <input type="button" ng-click="tab = 4" value="Deficiency Template" class="tabBtn" />
                                <input type="button" ng-click="tab = 5" value="Deficiency Reason" class="tabBtn" />
                                <input type="button" ng-click="tab = 6" value="Application Configuration" class="tabBtn" />
                                <input type="button" ng-click="tab = 7" value="Application Fee" class="tabBtn" />
                                <input type="button" ng-click="tab = 8" value="User Task" class="tabBtn" />
                        </div>
                        <div ng-show="tab === 1" CssClass="fancy fancy-green"> </div>
                        <div ng-show="tab === 2" CssClass="fancy fancy-green"></div>
                        <div ng-show="tab === 3" CssClass="fancy fancy-green"> </div>
                        <div ng-show="tab === 4" CssClass="fancy fancy-green">
                            @Html.Partial("~/Areas/BackOffice/Views/Home/PartialView/DeficiencyTemplate.cshtml");
                        </div>
                        <div ng-show="tab === 5" CssClass="fancy fancy-green">  </div>
                        <div ng-show="tab === 6" CssClass="fancy fancy-green">  </div>
                        <div ng-show="tab === 7" CssClass="fancy fancy-green">  </div>
                        <div ng-show="tab === 8" CssClass="fancy fancy-green">  </div>

                    
                </div>
                <div class="ribbon" style="width: 100%">
                    <div class="holder peel-shadows Individual" style="width: 98.4%!important; margin-top: -15px!important;margin-left: 0px !important;">
                    </div>
                </div>
            </div>
        </div>
        
    </div>
</div>


